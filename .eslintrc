{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "import",
    "react",
    "jest"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.js"
      },
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    },
    "import/extensions": [
      ".js",
      ".jsx"
    ]
  },
  "rules": {
    "import/no-named-as-default": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  }
}
