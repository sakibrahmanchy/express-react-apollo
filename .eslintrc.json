{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
      "import",
      "react",
      "jest"
  ],
  "settings": {
      "import/resolver": {
          "webpack": {
              "config": "./webpack.config.js"
          },
        "node": {
          "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
      },
      "import/extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
      ]
  },
  "rules": {
      "import/no-named-as-default": 0,
      "import/extensions": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  }
}
